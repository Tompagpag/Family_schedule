<%= form_for(@family, @contact) do |f| %>
  <%= f.label first name %>
  <%= f.text_field :first_name %>
  <%= f.label last name %>
  <%= f.text_field :last_name %>
  <%= f.label email %>
  <%= f.text_field :email %>
  <%= f.label phone number %>
  <%= f.number_field :phone_number %>
  <%= f.label comment %>
  <%= f.text_field :comment %>
  <%= f.submit %>
<% end %>


